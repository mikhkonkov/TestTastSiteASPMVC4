<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrap/css", "~/Content/css")
    <link href="/Content/bootstrap/css/bootstrap-login.css" rel="stylesheet" />
    <link href="/Content/bootstrap/css/datepicker.css" rel="stylesheet" />

</head>
<body>

    @Html.Partial("menu")

    <div class="container">
        @RenderBody()
    </div>

    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
    <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/bootstrap/js/bootstrap-datepicker.js"></script>
    <script>
        $(function () {
            $('#dp').datepicker();
        });
    </script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
</body>
</html>
